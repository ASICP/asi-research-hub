run = "python app.py"
modules = ["python-3.10"]

[nix]
channel = "stable-23_11"

[env]
PYTHONPATH = "$PYTHONPATH:$REPL_HOME"

[deployment]
run = ["sh", "-c", "gunicorn -w 4 -b 0.0.0.0:$PORT app:app"]
deploymentTarget = "cloudrun"

[[ports]]
localPort = 5000
externalPort = 80

[agent]
expertMode = true
